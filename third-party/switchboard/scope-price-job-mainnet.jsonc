                                    "cronParseTask": {
                                        "cronPattern": "* * * * * *",
                                        "clockOffset": 0,
                                        "clock": 1
                                    }
                                }
                            ]
                        }
                    }
                ]
            }
        },
        {
            "anchorFetchTask": {
                "programId": "HFn8GnPADiny6XqUoWE8uRPPxb29ikn4yTuPa9MF2fWJ", /* mainnet here */
                "accountAddress": "3NJYftD5sjVfxSnUdZ1wVML8f3aC6mp1CXCL6L7TnU8C"
            }
        },
        {
            "conditionalTask": {
                "attempt": [
                    {
                        "jsonParseTask": {
                            "path": "$.prices[?((@.idx==\"42\") && (@.unix_timestamp > (${CURRENT_TIMESTAMP} - 70)))].price.value"
                        }
                    },
                    {
                        "divideTask": {
                            "scalar": 100000000
                        }
                    }
                ],
                "onFailure": [
                    /** Nothing is done here if timestamp is less than the current timestamp */
                ]
            }
        }
    ]
